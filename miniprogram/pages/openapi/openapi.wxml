<!--index.wxml-->
<view class="container">

  <view class="uploader">
    <navigator url="./serverapi/serverapi" open-type="navigate" class="uploader-text">
      <text>服务端调用</text>
    </navigator>
  </view>

  <view class="uploader">
    <navigator url="./cloudid/cloudid" open-type="navigate" class="uploader-text">
      <text>开放数据调用</text>
    </navigator>
  </view>

  <view class="uploader">
    <navigator url="./callback/callback" open-type="navigate" class="uploader-text">
      <text>消息推送</text>
    </navigator>
  </view>

  <view class="uploader">
    <view bindtap="getTemplateList" class="uploader-text">
      <text>获取当前帐号下的个人模板列表</text>
    </view>
  </view>

</view>
